    {
      "parameters": {
        "name": "score_and_rank",
        "description": "Calculate weighted scores for business models based on revenue potential, automation feasibility, time-to-revenue, and risk. Returns ranked list.",
        "language": "javaScript",
        "jsCode": "// Score business models\nconst models = JSON.parse($input.first().json.models || '[]');\nconst scored = models.map(m => ({\n  ...m,\n  compositeScore: (\n    (m.revenuePotential / 10000) * 0.35 +\n    (m.automationFeasibility / 10) * 0.35 +\n    (1 - m.timeToRevenue / 12) * 0.20 +\n    (1 - m.riskScore / 10) * 0.10\n  )\n}));\nscored.sort((a, b) => b.compositeScore - a.compositeScore);\nreturn { rankedModels: scored };"
      },
